{
  "name": "@getlang/parser",
  "version": "0.0.17",
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    ".": {
      "bun": "./index.ts",
      "default": "./dist/index.js"
    },
    "./ast": {
      "bun": "./ast/ast.ts",
      "default": "./dist/ast/ast.js"
    },
    "./visitor": {
      "bun": "./ast/visitor.ts",
      "default": "./dist/ast/visitor.js"
    },
    "./scope": {
      "bun": "./ast/scope.ts",
      "default": "./dist/ast/scope.js"
    },
    "./typeinfo": {
      "bun": "./ast/typeinfo.ts",
      "default": "./dist/ast/typeinfo.js"
    }
  },
  "bugs": {
    "url": "https://github.com/getlang-dev/get/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getlang-dev/get.git",
    "directory": "packages/parser"
  },
  "homepage": "https://getlang.dev",
  "scripts": {
    "compile": "nearleyc parse/getlang.ne -o grammar.ts",
    "railroad": "nearley-railroad parse/getlang.ne -o grammar.html && open grammar.html"
  },
  "dependencies": {
    "@getlang/lib": "workspace:^0.0.13",
    "acorn": "^8.12.0",
    "acorn-globals": "^7.0.1",
    "globals": "^15.6.0",
    "moo": "^0.5.2",
    "nearley": "^2.20.1",
    "prettier": "^3.3.2"
  },
  "devDependencies": {
    "@types/moo": "^0.5.9",
    "@types/nearley": "^2.11.5"
  }
}
